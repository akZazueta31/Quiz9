print("First position")
x1=int(input("Give me the value of x= "))
y1=int(input("Give me the value of y= "))

print("Second position")
x2=int(input("Give me the value of x= "))
y2=int(input("Give me the value of y= "))

def hypotenuse(x1,x2,y1,y2):
	if(x1>x2):
		a=x1-x2
	else:
		a=x2-x1
	if(y1>y2):
		b=y1-y2
	else:
		b=y2-y1
	import math
	c=math.sqrt(a**2+b**2)
	return c

ans=hypotenuse(x1,x2,y1,y2)
print("The distance between both positions is: ", ans)
